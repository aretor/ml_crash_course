}\n","class_names = image_datasets['train'].classes\n","\n","\n","for param in cnn.parameters():  # freeze the weights\n","    param.requires_grad = False\n","\n","# Here the size of each output sample is set to 2.\n","# Alternatively, it can be generalized to nn.Linear(num_ftrs, len(class_names)).\n","vgg16.classifier[-1] = nn.Linear(4096, 2)\n","\n","optimizer = optim.SGD(vgg16.parameters(), lr=0.001, momentum=0.9)\n","trainer = Trainer(vgg16, loss, optimizer, dataloaders['train'],\n","                  dataloaders['val'], device)\n","trainer.train(epochs=11)"],"execution_count":null,"outputs":[{"output_type":"stream","text":["/usr/local/lib/python3.6/dist-packages/ipykernel_launcher.py:32: TqdmDeprecationWarning: This function will be removed in tqdm==5.0.0\n","Please use `tqdm.notebook.tqdm` instead of `tqdm.tqdm_notebook`\n"],"name":"stderr"},{"output_type":"display_data","data":{"application/vnd.jupyter.widget-view+json":{"model_id":"ac02be92c8124b0d80d20ff621e9d631","version_minor":0,"version_major":2},"text/plain":["HBox(children=(FloatProgress(value=0.0, description='EVAL', max=39.0, style=ProgressStyle(description_width='i…"]},"metadata":{"tags":[]}},{"output_type":"stream","text":["\n","ACC at start: 0.49673202614379086\n"],"name":"stdout"},{"output_type":"stream","text":["/usr/local/lib/python3.6/dist-packages/ipykernel_launcher.py:17: TqdmDeprecationWarning: This function will be removed in tqdm==5.0.0\n","Please use `tqdm.notebook.tqdm` instead of `tqdm.tqdm_notebook`\n"],"name":"stderr"},{"output_type":"display_data","data":{"application/vnd.jupyter.widget-view+json":{"model_id":"c177d75a51924b539d49a8828afe74c2","version_minor":0,"version_major":2},"text/plain":["HBox(children=(FloatProgress(value=0.0, description='TRAIN', max=61.0, style=ProgressStyle(description_width='…"]},"metadata":{"tags":[]}},{"output_type":"stream","text":["\n"],"name":"stdout"},{"output_type":"display_data","data":{"application/vnd.jupyter.widget-view+json":{"model_id":"702a